{
    "name": "vulcdataformat",
    "displayName": "VulcDataFormat",
    "description": "Language support for Textual VDF",
    "version": "1.1.0",
    "engines": {
        "vscode": "^1.51.0"
    },
    "publisher": "Vulcalien",
    "icon": "images/icon.png",
    "homepage": "https://github.com/Vulcalien/vscode-vulcdataformat-language",
    "repository": {
        "type": "git",
        "url": "https://github.com/Vulcalien/vscode-vulcdataformat-language"
    },
    "license": "SEE LICENSE IN LICENSE",
    "contributes": {
        "languages": [
            {
                "id": "vdf",
                "aliases": [
                    "VDF",
                    "vulcdataformat"
                ],
                "extensions": [
                    ".vdf",
                    ".tvdf"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "vdf",
                "scopeName": "source.vdf",
                "path": "./syntaxes/vdf.tmLanguage.json"
            }
        ]
    }
}
